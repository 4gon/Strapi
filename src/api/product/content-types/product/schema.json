{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Products",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true,
      "unique": false,
      "minLength": 3,
      "maxLength": 50
    },
    "Description": {
      "type": "richtext"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "availableQty": {
      "type": "integer",
      "default": 0,
      "required": true
    },
    "createdDate": {
      "type": "date",
      "required": true
    },
    "productId": {
      "type": "uid",
      "required": true
    }
  }
}
